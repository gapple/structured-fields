{
    "name": "gapple/structured-headers",
    "description": "Parser for Structured Headers for HTTP",
    "type": "library",
    "license": [
        "MIT",
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Geoff Appleby"
        }
    ],
    "support": {
        "source": "https://github.com/gapple/structured-headers",
        "issues": "https://github.com/gapple/structured-headers/issues"
    },
    "autoload": {
        "psr-4": {
            "gapple\\StructuredHeaders\\": "src/",
            "gapple\\Tests\\StructuredHeaders\\": "test/"
        }
    },
    "scripts": {
        "phpcs": "phpcs"
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "httpwg/structured-header-tests": "*@dev",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "httpwg/structured-header-tests",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/httpwg/structured-header-tests/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "git@github.com:httpwg/structured-header-tests.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
